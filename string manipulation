str1 = input("enter first string ")
str2 = input("enter two string ")
li1 = list(str1)
li2 = list(str2)
li5 = list(str1)
li6 = list(str2)

print(len(li1), len(str2)) 
print(str1+str2)
li3 = list()
li3=li1[0]
li1[0]=li2[0]
li2[0]=li3[0]

str3 = ''.join(li1)
str4 = ''.join(li2)
print(str3 , str4)

li4 = list()
li4=li5[-1]
li5[-1]=li6[-1]
li6[-1]=li4[0]

str5 = ''.join(li5)
str6 = ''.join(li6)
print(str5 , str6)
